# ADD r
87 add a, a
 0 m1_fetch
 0   fetch 87 at 0000
 0     get_pc_on_fetch 0000
 0     set_addr_bus 0000 -> 0000
 2     get_ir_on_refresh 0000
 2     set_addr_bus 0000 -> 0000
 4     set_pc_on_fetch 0000 -> 0001
 4   set_r 00 -> 01
 4 get_a 00
 4 get_a 00
 4 set_a 00 -> 00
 4 set_f 00 -> 40
 4 done

# ADD A, n
